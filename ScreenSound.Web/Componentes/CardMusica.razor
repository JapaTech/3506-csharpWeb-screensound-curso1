<MudCard>
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h6">@Musica!.nome</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
        <MudText Typo="Typo.body2">Duração: @Musica!.anoLancamento</MudText>
        <MudText Typo="Typo.body2">Artista: @Musica!.artistaNome</MudText>
        @* <MudList>
            @foreach (var item in Musica!.Genero)
            {
                item.Nome;
            }
        </MudList> *@
    </MudCardContent>
</MudCard>

@code {
    [Parameter]
    public MusicaResponse? Musica { get; set; }
}